{
  "package": "com.vivo.cardDemo",
  "name": "{{ appName }}",
  "versionName": "1.0.0",
  "versionCode": "1",
  "config": {                                               
    "logLevel": "debug",
    "designWidth": 720
  },
  "router": {
    "widgets": {
      "Card": {
        "name": "Jovi卡片示例-小卡片",
        "description": "Jovi小卡片示例",
        "component": "index",
        "path": "/Card",
        "features": [
          {
            "name": "system.router"
          }
        ],
        "params": {
          "title": "noTitle",
          "height": "23.7%",
          "enableFold": "false"
        },
        "minPlatformVersion": 1032,
        "targetManufactorys": [
          "vivo"
        ]
      },
      "FoldCard": {
        "name": "Jovi卡片示例-折叠卡片",
        "description": "Jovi折叠卡片示例",
        "component": "index",
        "path": "/FoldCard",
        "features": [
          {
            "name": "system.router"
          }
        ],
        "params": {
          "title": "noTitle",
          "height": "23.7%",
          "enableFold": "true"
        },
        "minPlatformVersion": 1032,
        "targetManufactorys": [
          "vivo"
        ]
      }
    }
  }
}